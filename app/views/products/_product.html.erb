<div id="<%= dom_id product %>">
  <p>
    <strong>Name:</strong>
    <%= product.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= product.description %>
  </p>

  <p>
    <strong>Manufacturer:</strong>
    <%= product.manufacturer %>
  </p>

  <p>
    <strong>Prescribable:</strong>
    <%= product.prescribable %>
  </p>

  <p>
    <strong>Img url:</strong>
    <%= product.img_url %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= product.category_id %>
  </p>

</div>
